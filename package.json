{
	"publisher": "JeremyBosscha",
	"name": "caress-generator",
	"displayName": "caress-generator",
	"description": "Generator for ionic modules/views/services etc",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.caress-generator"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.caress-generator",
			"title": "MijnCaress Generator"
		}],
		"viewsContainers": {
			"activitybar": [{
				"id": "caress-generator",
				"title": "MijnCaress Generator",
				"icon": "resources/care.svg",
				"command": "extension.caress-generator"
			}]
		},
		"views": {
			"caress-generator": [{
				"id": "extension.caress-generator",
				"name": "extension.caress-generator",
				"command": "extension.caress-generator"
			}]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.0-next.0"
	}
}